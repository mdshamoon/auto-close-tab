{
  "manifest_version": 2,

  "name": "Auto Close Tab",
  "description": "This extension automatically closes an inactive tab in 10 minutes",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },

  "permissions": ["tabs", "activeTab", "storage"]
}
