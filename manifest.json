{
  "manifest_version": 3,
  "name": "Auto Close Tab",
  "description": "This extension automatically closes an inactive tab in the time specified by the user. Default is 5 minutes",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/act.png",
      "32": "/images/act.png",
      "48": "/images/act.png",
      "128": "/images/act.png"
    }
  },

  "permissions": ["tabs", "activeTab", "storage", "alarms", "notifications"]
}
